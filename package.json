{
  "name": "debot",
  "version": "1.0.0",
  "description": "Example of music bot with discordx",
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js",
    "bot",
    "discord",
    "typescript"
  ],
  "homepage": "https://github.com/teperie/discord-bot#readme",
  "bugs": {
    "url": "https://github.com/teperie/discord-bot/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teperie/discord-bot.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "prettier": "prettier . --write",
    "start": "node dist/index.js",
    "watch": "nodemon --exec npm run dev --watch src --ext ts"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.17.0",
    "@discordx/importer": "^1.3.1",
    "@discordx/music": "^6.2.0",
    "@discordx/pagination": "^3.5.4",
    "discord.js": "^14.15.3",
    "discordx": "^11.11.1",
    "ffmpeg-static": "^5.2.0",
    "isomorphic-unfetch": "^4.0.2",
    "libsodium-wrappers": "^0.7.13",
    "spotify-url-info": "^3.2.15",
    "youtube-sr": "^4.3.11"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.4",
    "esbuild": "^0.23.0",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2",
    "rollup": "^4.18.1",
    "rollup-plugin-node-externals": "^7.1.2",
    "ts-node": "^10.9.2",
    "typescript": "5.4.5"
  }
}
