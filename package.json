{
  "name": "debot",
  "version": "1.0.0",
  "description": "Example of music bot with discordx",
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js",
    "bot",
    "discord",
    "typescript"
  ],
  "homepage": "https://github.com/teperie/discord-bot#readme",
  "bugs": {
    "url": "https://github.com/teperie/discord-bot/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teperie/discord-bot.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "tsx app.ts",
    "prepare": "husky",
    "prettier": "prettier . --write",
    "start": "node dist/index.js",
    "watch": "tsx watch app.ts"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.17.0",
    "@discordx/importer": "^1.3.1",
    "@discordx/pagination": "^3.5.4",
    "discord.js": "^14.15.3",
    "discordx": "^11.11.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.14.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "rollup": "^4.18.1",
    "rollup-plugin-node-externals": "^7.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "5.4.5"
  }
}
